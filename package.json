{
  "name": "neumorphic-proxy-generator",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron ./dist/main.js",
    "build:main": "tsc",
    "build:render": "webpack",
    "build:start": "tsc && webpack &&  electron ./dist/main.js",
    "package:mac": "tsc && webpack && electron-builder --mac"
  },
  "build": {
    "asar": true,
    "asarUnpack": "./squid",
    "extraMetadata": {
      "main": "./dist/main.js"
    },
    "extends": null,
    "productName": "Neumorphic Proxy Generator",
    "appId": "neumorphic.proxy.generator",
    "directories": {
      "output": "./product"
    },
    "files": [
      "./dist/**/*.js",
      "./dist/**/*.map",
      "./dist/index.html",
      "./main.js",
      "./squid/squid.conf"
    ],
    "mac": {
      "target": {
        "target": "dir",
        "arch": [
          "arm64",
          "x64"
        ]
      }
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/randomstring": "^1.1.6",
    "@types/styled-components": "^5.1.7",
    "clean-webpack-plugin": "^3.0.0",
    "connect-history-api-fallback": "^1.6.0",
    "electron-log": "^4.3.1",
    "eslint-loader": "^4.0.2",
    "koa-connect": "^2.1.0",
    "node-ssh": "^11.1.1",
    "randomstring": "^1.1.5",
    "react": "^16.8.0",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.1",
    "react-is": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "styled-components": "^5.2.1",
    "styled-react-modal": "^2.0.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.31",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "eslint": "^7.17.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^4.5.1",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1"
  }
}
